{
    "manifest_version": 3,
    "name": "Quick YouTube",
    "version": "1.0",
    "description": "Quickly open YouTube in a new tab",
    "permissions": [
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "commands": {
        "open-youtube": {
            "suggested_key": {
                "default": "Ctrl+Shift+Y"
            },
            "description": "Open YouTube in a new tab"
        }
    }
}